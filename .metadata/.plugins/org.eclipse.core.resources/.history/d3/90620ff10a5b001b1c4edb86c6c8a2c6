package com.test;

import java.util.Scanner;

public class Test {
	static public void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		
		System.out.print("당신의 이름을 입력하시오:");
		String name= sc.nextLine();
		
		
		System.out.print("당신의 키 입력:");
		double height=sc.nextDouble();
		
		System.out.print("당신의 성별을 적어주세요:");
		String gender=sc.next();
		
		System.out.print("당신의 주소를 말해주세요:");
		String address=sc.next();
		
		System.out.print("당신의 이름은 "+name+" 입니다"+" 당신의 키는 "+height+" 입니다"+" 당신의 성별은 "+gender+" 입니다"+" 당신의 주소는 "+address+" 입니다");
		
		
				
		
		
		
		
		
		
	}


}
