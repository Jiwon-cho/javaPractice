import java.util.*;

public class BankFunc {
	int amount;
	
	public BankFunc() {
		System.out.println(this.amount);
	}
	//입금
	public int addAmount(int var) {
		this.amount = this.amount+var;
	}
	//출금
	public int minusAmout(int var) {
		this.amount = this.amount-var;
	}
	public void wa() {
		System.out.println(this.amount);
	}
}
