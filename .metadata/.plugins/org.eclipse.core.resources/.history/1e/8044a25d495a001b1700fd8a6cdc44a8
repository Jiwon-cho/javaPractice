package com.vari.controller;

public class VariableUse {
	public static void main(String[] args) {
		
			
			
			
			int age;
			age=10;
			String c="abcc";
			long a = 1000;
		    a = 111111111111111111L;
		    
		    double dnum;
		    dnum=180.15;
		    float fnum;
		    fnum=189.5f;
		    
		    byte bnum;
		    bnum=100;
		    short snum;
		    snum=-1000;
		    
		    char cho;
		    cho='A';
		    
		    String str;
		    str="Askj";
		    System.out.println(str);
		    System.out.println(bnum);
		    
		    int grade,record,number,group;
		    
		    double height=190.5,weight=5123.5,avg=15151.1;
		    
		   //대입할 땐 리터럴, 변수도 대입이 가능 
		    int num2;
		    age=19;
		    num2=age;//num2=10
		    
			
					
			
			
			System.out.println(age+num2);
			String nickname="유인나";
			System.out.println(nickname);
			
			
			final int Age= 100;
			final String ADMIN_ID = "마이 묵었따 아이가";
			
		
			System.out.println(Age);
			System.out.println(ADMIN_ID);
			
			String msg;
			msg="쉬고싶다..";
			System.out.println(msg);
			msg=new String("제바라");
			System.out.println(msg);
		
	}
	

}
